{
  "env": {
      "es6": true,
      "node": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": [
      "@typescript-eslint"
  ],
  "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module",
      "impliedStrict": true,
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "extends": [
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}